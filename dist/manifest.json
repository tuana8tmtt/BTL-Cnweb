{
  "name": "ASV Toolbox",
  "version": "1.25",
  "description": "Complete toolbox set for ASV Software customers",
  "key": "",
  "manifest_version": 2,
  "permissions": [
    "tabs",
    "https://*.facebook.com/*",
    "https://audinsights.app/*",
    "http://localhost:3000/*",
    "cookies",
    "webRequest",
    "webRequestBlocking"
  ],
  "icons": {
    "16": "16.png",
    "32": "32.png",
    "64": "64.png",
    "128": "128.png"
  },
  "browser_action": {
    "default_icon": "128.png",
    "default_title": "ASV Toolbox"
  },
  "background": {
    "scripts": ["interceptor.js", "api.js"]
  },
  "externally_connectable": {
    "matches": [
      "https://autotarget.asvtool.com/*",
      "https://audinsights.app/*",
      "http://localhost:3000/*"
    ]
  }
}
